<div class="page-header">
  <h1>Import Students</h1>
</div>

<%= form_tag import_students_path, multipart: true, :html => {:class => 'form-horizontal'} do %>
    <div class="control-group">
      <%= label_tag :file, 'File' ,html: {:class => 'control-label' } %>
      <div class="controls">
        <%= file_field_tag :file %>
      </div>
    </div>
    <%= submit_tag "Import Student" %>
<% end %>
