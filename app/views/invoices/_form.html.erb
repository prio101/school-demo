<%= form_for @invoice, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :fee_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fee_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :school_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :school_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :title, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :title, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :state, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :state, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :user_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :user_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :voided_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :voided_at, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :paid_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :paid_at, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :due_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :due_at, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :course_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :course_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :batch_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :batch_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :amount, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :amount, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :invoice_number, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :invoice_number, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                invoices_path, :class => 'btn' %>
  </div>
<% end %>
